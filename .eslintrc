{
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "commonjs": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": [
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": "off",
    "import/no-cycle": "off",
    "no-console": "off"
  }
}